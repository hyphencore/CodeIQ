	public static void main(String[] args) {
		Scanner cin = new Scanner(System.in);
		int rest;

		while (cin.hasNext()) {
			rest = Integer.parseInt(cin.nextLine());
			System.out.println(getRyougaeMaisu(rest));
		}
		cin.close();
	}

}
